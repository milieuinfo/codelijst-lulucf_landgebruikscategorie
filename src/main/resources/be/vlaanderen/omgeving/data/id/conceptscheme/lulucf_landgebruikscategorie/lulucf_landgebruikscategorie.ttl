    skos:definition "All developed land, including transport infrastructure and residential areas of any size, unless they already fall under other categories such as forest, cropland, grassland or wetland (Eggleston et al., 2006)."@nl;
    skos:inScheme cat:lulucf_landgebruikscategorie;
    skos:narrower <https://data.omgeving.vlaanderen.be/id/concept/lulucf_landgebruikscategorie/501>, <https://data.omgeving.vlaanderen.be/id/concept/lulucf_landgebruikscategorie/502>;
